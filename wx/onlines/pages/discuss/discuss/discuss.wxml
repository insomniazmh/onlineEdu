<swiper
 indicator-color="#fff"
 indicator-dots="{{indicatorDots}}"
 autoplay="{{autoplay}}"
 interval="{{interval}}"
 duration="{{duration}}"
>
	<block wx:for="{{imgUrls}}" wx:key="disc">
		<swiper-item>
			<image
       style='width:100%'
			 src="{{item}}"
			 class="slide-image"
			 width="355"
			 height="150"
			/>
		</swiper-item>
	</block>
</swiper>
